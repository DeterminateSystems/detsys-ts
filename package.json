{
  "name": "detsys-ts",
  "version": "1.0.0",
  "description": "TypeScript goodies for DetSys projects",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "check-fmt": "biome format",
    "format": "biome format --write",
    "lint": "biome lint",
    "docs": "typedoc",
    "test": "vitest --watch false",
    "test-dev": "vitest",
    "all": "rm -rf dist && pnpm install && pnpm run format && pnpm run lint && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeterminateSystems/detsys-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DeterminateSystems/detsys-ts/issues"
  },
  "homepage": "https://github.com/DeterminateSystems/detsys-ts#readme",
  "dependencies": {
    "@actions/cache": "^4.0.3",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "got": "^14.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
